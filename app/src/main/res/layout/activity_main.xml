<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#303239"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="224dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rectangle8" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="4dp"
        android:visibility="visible"
        app:layout_constraintEnd_toStartOf="@+id/reminderbutton"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rectangle8" />

    <ImageButton
        android:id="@+id/foodbutton"
        android:layout_width="171dp"
        android:layout_height="133dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="192dp"
        android:adjustViewBounds="false"
        android:background="#00000000"
        android:backgroundTint="#00000000"
        android:baselineAlignBottom="false"
        android:clickable="false"
        android:contextClickable="true"
        android:cropToPadding="false"
        android:requiresFadingEdge="horizontal"
        android:scaleType="centerCrop"
        android:textAlignment="center"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rectangle4"
        tools:visibility="visible" />

    <ImageButton
        android:id="@+id/timetablebutton"
        android:layout_width="171dp"
        android:layout_height="133dp"
        android:layout_marginTop="192dp"
        android:layout_marginEnd="15dp"
        android:background="#00FFFFFF"
        android:backgroundTint="#00C81919"
        android:foregroundTint="#75AF1D1D"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rectangle5" />

    <ImageButton
        android:id="@+id/feedbackbutton"
        android:layout_width="372dp"
        android:layout_height="135dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="345dp"
        android:layout_marginEnd="15dp"
        android:background="#00FFFFFF"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rectangle6" />

    <ImageButton
        android:id="@+id/hostelbutton"
        android:layout_width="381dp"
        android:layout_height="135dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="498dp"
        android:layout_marginEnd="15dp"
        android:background="#00FFFFFF"
        android:contentDescription="biasdfasd"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rect_7" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginTop="2dp"
        android:fontFamily="@font/mont_semibold"
        android:text="TIMETABLE"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@id/timetablebutton"
        app:layout_constraintEnd_toEndOf="@id/timetablebutton"
        app:layout_constraintStart_toStartOf="@id/timetablebutton"
        app:layout_constraintTop_toTopOf="@id/timetablebutton"
        app:layout_constraintVertical_bias="0.508" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/mont_semibold"
        android:text="TIMETABLE"
        android:textColor="#FAFAFA"
        app:layout_constraintBottom_toBottomOf="@id/timetablebutton"
        app:layout_constraintEnd_toEndOf="@id/timetablebutton"
        app:layout_constraintStart_toStartOf="@id/timetablebutton"
        app:layout_constraintTop_toTopOf="@id/timetablebutton"
        app:layout_constraintVertical_bias="0.508" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="86dp"
        android:layout_height="22dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="2dp"
        android:fontFamily="@font/mont_semibold"
        android:text="FEEDBACK"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@id/feedbackbutton"
        app:layout_constraintEnd_toEndOf="@id/feedbackbutton"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="@id/feedbackbutton"
        app:layout_constraintTop_toTopOf="@id/feedbackbutton"
        app:layout_constraintVertical_bias="0.543" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="86dp"
        android:layout_height="22dp"
        android:fontFamily="@font/mont_semibold"
        android:text="FEEDBACK"
        android:textColor="#FAFAFA"
        app:layout_constraintBottom_toBottomOf="@id/feedbackbutton"
        app:layout_constraintEnd_toEndOf="@id/feedbackbutton"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="@id/feedbackbutton"
        app:layout_constraintTop_toTopOf="@id/feedbackbutton"
        app:layout_constraintVertical_bias="0.543" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="91dp"
        android:layout_height="26dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="2dp"
        android:fontFamily="@font/mont_semibold"
        android:text="CAB SHARE"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@id/hostelbutton"
        app:layout_constraintEnd_toEndOf="@id/hostelbutton"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toStartOf="@id/hostelbutton"
        app:layout_constraintTop_toTopOf="@id/hostelbutton"
        app:layout_constraintVertical_bias="0.56" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="91dp"
        android:layout_height="26dp"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="2dp"
        android:fontFamily="@font/mont_semibold"
        android:text="CAB SHARE"
        android:textColor="#FAFAFA"
        app:layout_constraintBottom_toBottomOf="@id/hostelbutton"
        app:layout_constraintEnd_toEndOf="@id/hostelbutton"
        app:layout_constraintStart_toStartOf="@id/hostelbutton"
        app:layout_constraintTop_toTopOf="@id/hostelbutton"
        app:layout_constraintVertical_bias="0.568" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginTop="2dp"
        android:fontFamily="@font/mont_semibold"
        android:text="FOOD"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@id/foodbutton"
        app:layout_constraintEnd_toEndOf="@id/foodbutton"
        app:layout_constraintHorizontal_bias="0.481"
        app:layout_constraintStart_toStartOf="@id/foodbutton"
        app:layout_constraintTop_toTopOf="@id/foodbutton"
        app:layout_constraintVertical_bias="0.508" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/mont_semibold"
        android:text="FOOD"
        android:textColor="#FAFAFA"
        app:layout_constraintBottom_toBottomOf="@id/foodbutton"
        app:layout_constraintEnd_toEndOf="@id/foodbutton"
        app:layout_constraintHorizontal_bias="0.481"
        app:layout_constraintStart_toStartOf="@id/foodbutton"
        app:layout_constraintTop_toTopOf="@id/foodbutton"
        app:layout_constraintVertical_bias="0.508" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="427dp"
        android:layout_height="203dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rectangle8" />

    <ImageButton
        android:id="@+id/navbutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="24dp"
        android:background="#00FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/sort" />

    <ImageButton
        android:id="@+id/reminderbutton"
        android:layout_width="33dp"
        android:layout_height="32dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="20dp"
        android:background="#00FFFFFF"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/belldr" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="40dp"
        android:text="EXPLORE"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:fontFamily="@font/mont_black"/>

</androidx.constraintlayout.widget.ConstraintLayout>